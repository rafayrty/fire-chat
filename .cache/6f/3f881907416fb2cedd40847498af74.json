ow: inset 0px -1px 0px #E5E5EA;\r\n    display: flex;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    .avatars {\r\n        display: flex;\r\n        width: 26px;\r\n        height: auto;\r\n\r\n        .avatar {\r\n            position: relative;\r\n            border: 1px solid #FFF;\r\n            border-radius: 100%;\r\n\r\n            img {\r\n                display: block;\r\n            }\r\n        }\r\n\r\n        .avatar.second {\r\n            left: -10px;\r\n        }\r\n\r\n        .avatar.third {\r\n            left: -20px;\r\n        }\r\n\r\n        .avatar.fourth {\r\n            left: -30px;\r\n        }\r\n    }\r\n\r\n    .chat-name {\r\n        text-align: center;\r\n\r\n        h1 {\r\n            color: #2C2C2E;\r\n            font-weight: 600;\r\n            font-size: 0.83rem;\r\n        }\r\n\r\n        span {\r\n            color: #666666;\r\n            font-size: 0.75rem;\r\n        }\r\n    }\r\n}\r\n\r\nfooter {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: space-between;\r\n    position:absolute;\r\n    width:100%;\r\n    bottom:0;\r\n    box-shadow: inset 0px 1px 0px #E5E5EA;\r\n    z-index:5;\r\n    .input{\r\n        width:100%;\r\n        border:none;\r\n        padding:.25rem;\r\n        input{\r\n            width:100%;\r\n            padding:1rem;\r\n            border:none;\r\n            outline:none;\r\n            border:1px solid #FFF;\r\n            &:focus{\r\n                border:1px solid #007AFF;\r\n            }\r\n        }\r\n    }\r\n\r\n    .emoji{\r\n        padding-left:1rem;\r\n    }\r\n    .send-options{\r\n        padding-right:1rem;\r\n        display:flex;\r\n        .send{\r\n            margin-left:.8rem;\r\n        }\r\n    }\r\n\r\n}\r\n\r\nmain{\r\n    padding:1rem;\r\n    padding-bottom:0;\r\n    overflow:auto;\r\n    // height:100%;\r\n    // height:calc(105vh - 10.5rem);\r\n    .date{\r\n        margin-top:.4rem;\r\n        text-align: center;\r\n        font-size:12px;\r\n        color:#666666;\r\n    }\r\n\r\n    .chat-ui{\r\n        margin-top:1.4rem;\r\n        padding-bottom:1rem;\r\n        display:flex;\r\n        flex-direction: column;\r\n\r\n        .msg{\r\n            position:relative;\r\n          .msg-content{\r\n            color:#FFF;\r\n            width:fit-content;\r\n            padding:.7rem .5rem;\r\n            padding-bottom:.35rem;\r\n            .msg-text{\r\n                display:flex;\r\n            }\r\n          }\r\n\r\n            &-info{\r\n                display:flex;\r\n                justify-content: flex-end;\r\n                align-items: center;\r\n                margin-top:.5rem; \r\n                &__time{\r\n                    font-size:0.75rem;\r\n                }\r\n                &__receipt{\r\n                    margin-left:.5rem;\r\n                }\r\n            }\r\n            &:not(:first-child){\r\n                margin-top:1.2rem;\r\n            }\r\n        }\r\n        .msg.sender{\r\n            align-self: flex-end;\r\n            .msg-content{\r\n                background:#007AFF;\r\n                border-radius: 6px 0px 6px 6px;\r\n            }\r\n            #tip{\r\n                position:absolute;\r\n                right:-1.8%;\r\n                top:0;\r\n            }\r\n        }\r\n        .msg.receiver{\r\n            display:flex;\r\n            #tip{\r\n                position:absolute;\r\n                left:8.4%;\r\n                top:0;\r\n            }\r\n\r\n            .msg-content{\r\n                color:#2C2C2C;\r\n                background:#F2F2F7;\r\n                margin-left:1rem;\r\n                border-radius: 0px 6px 6px 6px;\r\n\r\n            }\r\n            .msg-info{\r\n                &__time{\r\n                    opacity:.6;\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media only screen and (max-width:768px){\r\n    .main-container{\r\n        width:100%;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"923ed56c9f25c5d989d3b9b92b5b2525","cacheData":{"env":{}}}